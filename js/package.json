{
  "name": "nina-workspace",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "nina-common",
    "soft-lp",
    "web"
  ],
  "scripts": {
    "nina-common": "yarn workspace nina-common",
    "soft-lp": "yarn workspace soft-lp",
