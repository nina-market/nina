{
  "name": "nina-workspace",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "nina-radio",
    "sdk",
    "web",
    "hubs"
  ],
  "scripts": {
    "sdk": "yarn workspace @nina-protocol/nina-internal-sdk",
    "nina-radio": "yarn workspace nina-radio",
    "nina-radio:build": "USE_KEY=true yarn nina-radio build",
    "web": "yarn workspace web",
    "web:build": "USE_KEY=true yarn web build",
    "hubs": "yarn workspace @nina-protocol/nina-hubs",
    "hubs:build": "USE_KEY=true yarn hubs build",
    "check-lint-format": "yarn hubs check-lint-format && yarn sdk check-lint-format && yarn web check-lint-format"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@imgix/js-core": "^3.6.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/system": "^4.11.3",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.6.3",
    "@mui/styles": "^5.6.2",
    "@nina-protocol/js-sdk": "^0.0.14",
    "@nina-protocol/nina-internal-sdk": "0.0.35",
    "@sentry/nextjs": "^7.8.0",
    "airtable": "^0.11.5",
    "ansi-regex": "^5.0.1",
    "env-cmd": "^10.1.0",
    "ga-gtag": "^1.1.1",
    "glob-parent": "^5.1.2",
    "nanoid": "^3.1.31",
    "next": "12.2.0",
    "node-fetch": "^2.6.7",
    "node-forge": "^1.0.0",
    "quill-magic-url": "^4.1.4",
    "react": "^17.0.2",
    "react-animated-ellipsis": "^1.0.0",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.5",
    "yup": "^0.32.11",
    "zlib": "^1.0.5",
    "zlib-browserify": "^0.0.3"
  },
  "browser": {
    "crypto": false,
    "fs": false
  }
}
